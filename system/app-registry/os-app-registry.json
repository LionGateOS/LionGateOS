{
  "version": "b29-security-center-ui-1",
  "registryPolicy": {
    "authority": "os",
    "evaluationOrder": [
      "accountState",
      "entitlementPolicy",
      "permissionEngine",
      "runtimeExecution"
    ],
    "defaultBehaviors": {
      "grace": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only"
      },
      "suspended": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only"
      },
      "canceled": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only-until-period-end"
      }
    }
  },
  "featureCaps": {
    "rows.ingest.documents": { "window": "30d", "limitsByTier": { "Guest": 1, "Free": 5, "Founder": 100, "Pro": 300, "Commercial": "contract" } },
    "rows.ingest.pages": { "window": "per-doc", "limitsByTier": { "Guest": 5, "Free": 20, "Founder": 100, "Pro": 200, "Commercial": "contract" } },
    "rows.emit.rows": { "window": "30d", "limitsByTier": { "Guest": 100, "Free": 1000, "Founder": 25000, "Pro": 100000, "Commercial": "contract" } },
    "rows.export": { "window": "30d", "limitsByTier": { "Guest": 1, "Free": 5, "Founder": "unlimited", "Pro": "unlimited", "Commercial": "unlimited" } },

    "budget.workspaces": { "window": "account", "limitsByTier": { "Free": 1, "Founder": 1, "Pro": 3, "Commercial": "org" } },
    "budget.datasets": { "window": "account", "limitsByTier": { "Free": 3, "Founder": "unlimited", "Pro": "unlimited", "Commercial": "unlimited" } },

    "travels.trips": { "window": "account", "limitsByTier": { "Free": 1, "Founder": "unlimited", "Pro": "unlimited", "Commercial": "unlimited" } },

    "documents.generate": { "window": "30d", "limitsByTier": { "Free": "limited", "Founder": "unlimited", "Pro": "unlimited", "Commercial": "unlimited" } },

    "estimator.basic": { "window": "30d", "limitsByTier": { "Free": "enabled", "Founder": "enabled", "Pro": "enabled", "Commercial": "enabled" } },
    "estimator.advanced": { "window": "30d", "limitsByTier": { "Pro": "enabled", "Commercial": "enabled" } },

    "smartquote.create": { "window": "30d", "limitsByTier": { "Pro": "enabled", "Commercial": "enabled" } },
    "smartquote.export": { "window": "30d", "limitsByTier": { "Pro": "enabled", "Commercial": "enabled" } },
    "smartquote.org": { "window": "org", "limitsByTier": { "Commercial": "enabled" } }
  },
  "apps": [
    {
      "id": "home",
      "name": "Home",
      "category": "system",
      "status": "stable",
      "runsInsideOs": true,
      "runsStandalone": false,
      "access": {
        "requiresLogin": false,
        "minAccountState": "Guest",
        "allowedStates": ["Guest", "Free", "Founder", "Pro", "Commercial", "Grace", "Suspended", "Canceled"],
        "behaviorByState": {
          "Guest": "full",
          "Grace": "full",
          "Suspended": "full"
        }
      },
      "features": [],
      "integration": {
        "osSlot": "workspace",
        "path": "home",
        "sandbox": false,
        "permissions": ["storage"]
      }
    },
    {
      "id": "ai",
      "name": "AI Workspace",
