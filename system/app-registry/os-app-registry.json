{
  "version": "b29-security-center-ui-1",
  "registryPolicy": {
    "authority": "os",
    "evaluationOrder": [
      "accountState",
      "entitlementPolicy",
      "permissionEngine",
      "runtimeExecution"
    ],
    "defaultBehaviors": {
      "grace": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only"
      },
      "suspended": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only"
      },
      "canceled": {
        "ingestion": "deny",
        "exports": "deny",
        "mode": "read-only-until-period-end"
      }
    }
  },
  "apps": [
    {
      "id": "home",
      "name": "Home",
      "category": "system",
      "status": "stable",
      "runsInsideOs": true,
      "runsStandalone": false,
      "access": {
        "requiresLogin": false,
        "minAccountState": "Guest",
        "allowedStates": ["Guest", "Free", "Founder", "Pro", "Commercial", "Grace", "Suspended", "Canceled"],
        "behaviorByState": {
          "Guest": "full",
          "Grace": "full",
          "Suspended": "full"
        }
      },
      "features": [],
      "integration": {
        "osSlot": "workspace",
        "path": "home",
        "sandbox": false,
        "permissions": ["storage"]
      }
    },
    {
      "id": "ai",
      "name": "AI Workspace",
      "category": "ai",
