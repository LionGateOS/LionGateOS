        "requiresLogin": true,
        "minAccountState": "Free",
        "allowedStates": ["Free", "Founder", "Pro", "Commercial", "Grace", "Suspended", "Canceled"],
        "behaviorByState": {
          "Grace": "read-only",
          "Suspended": "read-only"
        }
      },
      "features": [
        { "id": "ai.workspace", "requiredTier": "Free" }
      ],
      "integration": {
        "osSlot": "workspace",
        "path": "ai",
        "sandbox": true,
